
func OnlyIf() {
	if Find() {
		"CASE 1"
		"dfs kfja;lsdkfja;slkdfj a;lskdfj a;lskdjf a;lskdj fa;lskdjf asd"
		"sd faskdf asdfa dfa"
	}
}
		
@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
@ OnlyIf                      @
@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
              |                
              |                
IF ############################
# Find                        #
###############################
        |               |      
        |               |      
::::::::::::::::::: :::::::::::
: TRUE            : : FALSE   :
::::::::::::::::::: :::::::::::
        |               |      
        |               |      
:::::::::::::::::::     |      
: CASE 1          :     |      
:::::::::::::::::::     |      
        |               |      
        |               |      
:::::::::::::::::::     |      
: dfs kfja;lsdkfj :     |      
: a;slkdfj a;lskd :     |      
: fj a;lskdjf a;l :     |      
: skdj fa;lskdjf  :     |      
: asd             :     |      
:::::::::::::::::::     |      
        |               |      
        |               |      
:::::::::::::::::::     |      
: sd faskdf asdfa :     |      
:  dfa            :     |      
:::::::::::::::::::     |      
        |               |      
        |               |      
IF ############################
# End of if                   #
###############################
              |                
              |                
@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
@ End of OnlyIf               @
@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
