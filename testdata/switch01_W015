
func switch01() {
	switch "General note" {
	case "State 1":
		"SQ1"
	case "State 2":
		"SW1"
		"SW2"
	case "State 3":
		"SR1"
	default:
		"DF1"
	}
}
		
@@@@@@@@@@@@@@@
@ SWITCH01    @
@@@@@@@@@@@@@@@
      |        
SWITCH $$$$$$$$
$ GENERAL NOT $
$ E           $
$$$$$$$$$$$$$$$
      |        
IF ############
# STATE 1     #
###############
 |       |      
 |  ::::::::::::
 |  : SQ1      :
 |  ::::::::::::
 |       |      
IF ############
# STATE 2     #
###############
 |       |      
 |  ::::::::::::
 |  : SW1      :
 |  ::::::::::::
 |       |      
 |  ::::::::::::
 |  : SW2      :
 |  ::::::::::::
 |       |      
IF ############
# STATE 3     #
###############
 |       |      
 |  ::::::::::::
 |  : SR1      :
 |  ::::::::::::
 |       |      
IF ############
# DEFAULT CAS #
# E OF SWITCH #
###############
 |       |      
 |  ::::::::::::
 |  : DF1      :
 |  ::::::::::::
 |       |      
SWITCH $$$$$$$$
$ END OF SWIT $
$ CH          $
$$$$$$$$$$$$$$$
      |        
@@@@@@@@@@@@@@@
@ END OF SWIT @
@ CH01        @
@@@@@@@@@@@@@@@
