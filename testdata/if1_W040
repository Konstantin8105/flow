
func OnlyIf() {
	if Find() {
		"CASE 1"
		"dfs kfja;lsdkfja;slkdfj a;lskdfj a;lskdjf a;lskdj fa;lskdjf asd"
		"sd faskdf asdfa dfa"
	}
}
		
@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
@ ONLYIF                               @
@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
                   |                    
                   |                    
IF #####################################
# FIND                                 #
########################################
           |                    |       
           |                    |       
::::::::::::::::::::::::: ::::::::::::::
: TRUE                  : : FALSE      :
::::::::::::::::::::::::: ::::::::::::::
           |                    |       
           |                    |       
:::::::::::::::::::::::::       |       
: CASE 1                :       |       
:::::::::::::::::::::::::       |       
           |                    |       
           |                    |       
:::::::::::::::::::::::::       |       
: DFS KFJA;LSDKFJA;SLKD :       |       
: FJ A;LSKDFJ A;LSKDJF  :       |       
: A;LSKDJ FA;LSKDJF ASD :       |       
:::::::::::::::::::::::::       |       
           |                    |       
           |                    |       
:::::::::::::::::::::::::       |       
: SD FASKDF ASDFA DFA   :       |       
:::::::::::::::::::::::::       |       
           |                    |       
           |                    |       
IF #####################################
# END OF IF                            #
########################################
                   |                    
                   |                    
@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@
@ END OF ONLYIF                        @
